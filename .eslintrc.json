{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "rallycoding",
    "rules": {
        "arrow-body-style": [
            "error",
            "always"
        ],
        "eol-last": [
            "error",
            "always"
        ],
        "guard-for-in": 0,
        "no-restricted-syntax": 0,
        "no-param-reassign": ["error", {
            "props": false
        }]
    }
}
